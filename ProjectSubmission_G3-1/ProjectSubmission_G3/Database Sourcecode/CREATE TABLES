#ER: STUDENT
CREATE TABLE STUDENT ( StudentNum VARCHAR(9) NOT NULL,
SSN VARCHAR(11) NOT NULL,
FName VARCHAR(30) NOT NULL,
MInit CHAR(1),
LName VARCHAR(30) NOT NULL,
DOB DATE,
CurrPhone VARCHAR(15),
PermPhone VARCHAR(15),
Sex VARCHAR(20),
Class VARCHAR(15) NOT NULL,
Degree VARCHAR(15),
Street VARCHAR(60),
City VARCHAR(60),
State CHAR(2),
ZipCode INTEGER,
CurrAddress VARCHAR(255),
PRIMARY KEY (StudentNum, SSN));

#ER: DEPARTMENT
CREATE TABLE DEPARTMENT ( DeptName VARCHAR(30) NOT NULL,
Code VARCHAR(4) NOT NULL,
OfficeNum INTEGER,
OfficePhone VARCHAR(15),
College VARCHAR(30),
PRIMARY KEY (Code) );

#ER: Major_Minor_In
CREATE Major_Minor_In (MajorDept VARCHAR(4),
MinorDept VARCHAR(4),
StudentNum VARCHAR(9) NOT NULL,
PRIMARY KEY (StudentNum) );

#ER: INSTRUCTOR
CREATE TABLE INSTRUCTOR ( InstNum VARCHAR(9) NOT NULL,
SSN VARCHAR(11) NOT NULL,
FName VARCHAR(30) NOT NULL,
MInit CHAR(1),
LName VARCHAR(30) NOT NULL,
Age VARCHAR(3),
CurrPhone VARCHAR(15),
Address VARCHAR(255),
Office INTEGER,
PRIMARY KEY (InstNum) );

#ER: COURSE
CREATE TABLE COURSE ( CourseNum VARCHAR(8) NOT NULL,
CourseName VARCHAR(30) NOT NULL,
Description VARCHAR(255),
CreditHours INTEGER,
CourseLevel VARCHAR(30),
PRIMARY KEY (CourseNum) );

#ER: ENROLLED_IN
CREATE TABLE ENROLLED_IN ( StudentNum VARCHAR(9) NOT NULL,
CourseNum VARCHAR(8) NOT NULL,
SectionNum INTEGER NOT NULL,
Semester VARCHAR(6) NOT NULL,
Year CHAR(4) NOT NULL,
GradePoint FLOAT,
LetterGrade VARCHAR(2),
PRIMARY KEY (StudentNum, SectionNum, CourseNum, Semester, Year) ); 

#ER: SECTION
CREATE TABLE SECTION ( CourseNum VARCHAR(8) NOT NULL,
SectionNum INTEGER NOT NULL,
Semester VARCHAR(6) NOT NULL,
Year CHAR(4) NOT NULL,
PRIMARY KEY (CourseNum, SectionNum, Semester, Year) );

#ER: TEACHES
CREATE TABLE TEACHES ( InstNum VARCHAR(9) NOT NULL,
CourseNum VARCHAR(8) NOT NULL,
SectionNum INTEGER NOT NULL,
Semester VARCHAR(6) NOT NULL,
Year CHAR(4) NOT NULL,
PRIMARY KEY (InstNum, CourseNum, SectionNum, Semester, Year) );

#ER: MEMBER_OF
CREATE TABLE MEMBER_OF ( InstNum VARCHAR(9) NOT NULL,
DeptCode VARCHAR(4) NOT NULL,
PRIMARY KEY (InstNum, DeptCode) );

#ER: OFFERS
CREATE TABLE OFFERS ( DeptCode VARCHAR(4) NOT NULL,
CourseNum VARCHAR(8) NOT NULL,
PRIMARY KEY (DeptCode, CourseNum) );

#ER: PREREQUISITE
CREATE TABLE REQUIRES_PREREQUISITE ( CourseNum VARCHAR(8) NOT NULL,
PrereqNum VARCHAR(8),
PRIMARY KEY(CourseNum, PrereqNum) );


